			<xs:restriction base="xs:string">
				<xs:enumeration value="${bisdir}/backup/config/${package.name}"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="installedVersion">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="regKey"/>
				<xs:element ref="regParam"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
